{
  "name": "promagen-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "db:push": "prisma db push",
    "db:seed": "node dist/seed.js",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "6.16.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "pino": "^9.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.14.12",
    "prisma": "6.16.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.9.2"
  }
}
