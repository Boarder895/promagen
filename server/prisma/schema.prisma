datasource db {
  provider = "postgresql"     // or your actual provider
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

// Use a DB-safe enum identifier that does not start with a digit
enum Provider {
  openai
  stability
  leonardo
  artistly
  firefly
  ideogram
  midjourney
  bing
  canva
  nightcafe
  playground
  pixlr
  fotor
  openart
  deepai
  lexica
  novelai
  recraft
  I23RF     // ‚Üê renamed from 123RF (and from _123rf if you had it)
  flux
}

model ProviderKey {
  id        String   @id @default(cuid())
  userId    String
  provider  Provider
  apiKeyEnc String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([userId, provider])
}
