{
  "name": "promagen-frontend",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "clean": "pwsh -NoProfile -File scripts/clean.ps1",
    "start": "next start -p 3000",
    "dev": "next dev",
    "lint": "next lint",
    "typecheck": "tsc --noEmit --pretty false",
    "types:summary": "pwsh -File ./scripts/typecheck-summary.ps1",
    "check:app-structure": "pwsh -NoProfile -File scripts/check-app-structure.ps1",
    "check:placeholders": "pwsh -File ./scripts/check-placeholders.ps1",
    "check:placeholders:fix": "pwsh -File ./scripts/check-placeholders.ps1 -fix",
    "format": "prettier -w \"src/**/*.{ts,tsx,md,css}\"",
    "format:check": "prettier -c \"src/**/*.{ts,tsx,md,css}\"",
    "rebuild": "npm run clean && npm ci && npm run build",
    "rebuild:clean": "npm run clean && npm run build",
    "books:touch": "node -e \"console.log('Books dir ok')\""
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.90.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "fuse.js": "^6.6.2",
    "intl-segmenter-polyfill": "^0.4.4",
    "lucide-react": "^0.453.0",
    "luxon": "^3.7.2",
    "next": "14.2.13",
    "next-themes": "^0.4.6",
    "openai": "^4.104.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.15.4",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "server-only": "^0.0.1",
    "swr": "^2.3.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "unified": "^11.0.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/luxon": "^3.7.1",
    "@types/node": "^20.19.21",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "autoprefixer": "^10.4.21",
    "country-flag-icons": "^1.5.21",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.13",
    "eslint-config-prettier": "^10.1.8",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.18",
    "ts-api-utils": "^2.1.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": "22.x"
  }
}
