    "zod": "echo zod-schema-checks-placeholder || exit 0",
    "info": "next info"
  },
  "dependencies": {
    "lucide-react": "^0.544.0",
    "next": "^15.5.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "swr": "^2.3.6",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.20.5",
    "typescript": "^5.5.4"
  },
  "depcheck": {
    "ignores": [
      "typescript",
      "postcss",
      "autoprefixer",
      "tailwindcss"
