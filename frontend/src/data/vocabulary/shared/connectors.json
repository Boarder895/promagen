{
  "$schema": "./schemas/shared.schema.json",
  "version": "1.0.0",
  "meta": {
    "domain": "shared",
    "category": "connectors",
    "label": "Prompt Connectors",
    "description": "Linking words and phrases that connect prompt elements together",
    "totalOptions": 280,
    "updated": "2026-01-21"
  },
  "byFunction": {
    "featuring": {
      "label": "Featuring / Including",
      "description": "Introduces subjects or elements",
      "options": [
        "featuring", "with", "including", "containing", "showing",
        "depicting", "displaying", "presenting", "showcasing", "highlighting",
        "starring", "centered on", "focused on", "emphasizing", "spotlighting",
        "incorporating", "integrating", "comprising", "consisting of"
      ],
      "usage": "Connect main subject to secondary elements",
      "example": "portrait of a woman [featuring] flowing red hair"
    },
    "style_reference": {
      "label": "Style Reference",
      "description": "References artistic styles or artists",
      "options": [
        "in the style of", "styled like", "inspired by", "influenced by",
        "reminiscent of", "evocative of", "channeling", "echoing",
        "in the manner of", "after", "following", "à la",
        "paying homage to", "referencing", "drawing from", "borrowing from",
        "in the tradition of", "in the vein of", "akin to", "similar to",
        "modeled after", "patterned after", "based on", "derived from"
      ],
      "usage": "Link subject to artistic style or artist reference",
      "example": "landscape [in the style of] Monet"
    },
    "location": {
      "label": "Location / Setting",
      "description": "Places subjects in environments",
      "options": [
        "in", "inside", "within", "at", "on", "upon",
        "against", "before", "beside", "near", "by", "next to",
        "surrounded by", "amidst", "among", "amongst", "between",
        "overlooking", "facing", "toward", "towards",
        "set in", "placed in", "situated in", "located in", "positioned in",
        "standing in", "sitting in", "lying in", "floating in", "emerging from",
        "entering", "exiting", "passing through", "crossing"
      ],
      "usage": "Place subject in an environment",
      "example": "warrior [standing in] ancient ruins"
    },
    "background": {
      "label": "Background / Backdrop",
      "description": "Describes background elements",
      "options": [
        "against a backdrop of", "with a background of", "before a",
        "set against", "framed by", "backed by", "silhouetted against",
        "contrasted with", "complemented by", "accented by",
        "with distant", "with far-off", "with looming", "with towering",
        "beneath", "under", "below", "above", "over",
        "in front of", "behind", "beyond"
      ],
      "usage": "Describe what's behind the subject",
      "example": "portrait [against a backdrop of] stormy skies"
    },
    "lighting": {
      "label": "Lighting Connectors",
      "description": "Introduces lighting conditions",
      "options": [
        "lit by", "illuminated by", "bathed in", "drenched in", "soaked in",
        "glowing with", "radiating", "emanating", "casting",
        "under", "beneath", "in the light of", "by the light of",
        "with dramatic", "with soft", "with harsh", "with natural",
        "catching the light", "reflecting", "refracting", "diffracting",
        "backlit by", "sidelit by", "rimlit by", "spotlit by"
      ],
      "usage": "Connect subject to lighting conditions",
      "example": "figure [bathed in] golden sunlight"
    },
    "atmosphere": {
      "label": "Atmosphere / Mood",
      "description": "Introduces atmospheric conditions",
      "options": [
        "shrouded in", "enveloped in", "wrapped in", "cloaked in",
        "emerging from", "fading into", "dissolving into", "melting into",
        "through", "piercing through", "cutting through", "breaking through",
        "with an atmosphere of", "with a sense of", "evoking", "conveying",
        "exuding", "radiating", "projecting", "emanating"
      ],
      "usage": "Add atmospheric or mood elements",
      "example": "castle [shrouded in] mysterious fog"
    },
    "wearing": {
      "label": "Wearing / Clothing",
      "description": "Describes what subjects are wearing",
      "options": [
        "wearing", "dressed in", "clothed in", "clad in", "adorned in",
        "draped in", "wrapped in", "covered in", "outfitted in",
        "sporting", "donning", "bearing", "carrying",
        "with", "having", "possessing", "equipped with",
        "accessorized with", "decorated with", "embellished with", "ornamented with"
      ],
      "usage": "Describe clothing and accessories",
      "example": "woman [dressed in] flowing silk gown"
    },
    "action": {
      "label": "Action / Movement",
      "description": "Describes what subjects are doing",
      "options": [
        "while", "as", "in the act of", "in the process of", "in the midst of",
        "caught", "frozen", "captured", "depicted",
        "performing", "executing", "doing", "engaged in", "occupied with",
        "about to", "preparing to", "ready to", "poised to",
        "just finished", "having completed", "after"
