    {
      "id": "twelvedata.fx.exchange_rate",
      "provider_id": "twelvedata",
      "kind": "fx",
      "group": "ribbon",
      "role": "fx_exchange_rate_twelvedata",
      "description": "Real-time FX exchange rate (single pair) from Twelve Data.",
      "response_shape": "twelvedata.fx.exchange_rate_response",
      "data_shape": "single_quote",
      "http": {
        "method": "GET",
        "path": "/time_series",
        "query": {
          "symbol": "{pair}",
          "interval": "1min",
          "outputsize": "1"
        }
      },
      "default_update": {
        "cadence": "60s",
        "only_when_open": true,
        "jitter_seconds": 5
      },
      "quota_block_id": "twelvedata-core",
      "quota_cost": 1,
      "status": "active",
      "tags": ["fx", "backup"],
      "notes": "Backup FX quote endpoint (used if FMP FX quote fails)."
    },
