{
  "problemMatcher": [
    {
      "owner": "eslint-stylish",
      "pattern": [
        {
          "regexp": "^([^\\s].*):\\sline\\s(\\d+),\\scol\\s(\\d+),\\s(Error|Warning)\\s-\\s(.*)\\s\\((.*)\\)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5,
          "code": 6
        }
      ]
    }
  ]
}
