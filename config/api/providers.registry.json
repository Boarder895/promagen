{
  "version": 1,
  "providers": [
    {
      "id": "twelvedata",
      "name": "Twelve Data",
      "base_url": "https://api.twelvedata.com/price",
      "auth": {
        "type": "query_api_key",
        "location": "query",
        "field": "apikey",
        "env_var": "TWELVEDATA_API_KEY"
      },
      "quotas": {
        "per_second": 1,
        "per_minute": 8,
        "per_day": 800,
        "per_month": null
      },
      "capabilities": ["fx_ribbon"],
      "adapters": {
        "fx_quotes": "twelvedata_fx_v1"
      }
    }
  ]
}
