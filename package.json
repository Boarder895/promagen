{
  "name": "promagen",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "engines": { "node": ">=18 <21", "pnpm": ">=8.7" },
  "engineStrict": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "bootstrap": "pnpm -w install --frozen-lockfile",
    "check": "pnpm -r run check",
    "build": "pnpm -r run build",
    "test:ci": "pnpm -r run test:ci",
    "lint": "pnpm -r exec eslint \"src/**/*.{ts,tsx}\""
  }
}

